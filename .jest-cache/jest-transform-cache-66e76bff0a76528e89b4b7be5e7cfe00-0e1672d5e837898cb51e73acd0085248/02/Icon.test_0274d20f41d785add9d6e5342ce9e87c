'use strict';

import React from "react";
import { mount } from "enzyme";
import Icon from '../../../../web/src/js/components/Icon';

describe('Icon', function () {
  var props = void 0;
  var mountedIcon = void 0;

  var icon = function icon() {
    if (!mountedIcon) {
      mountedIcon = mount(
      React.createElement(Icon, props));

    }
    return mountedIcon;
  };

  beforeEach(function () {
    props = {
      name: undefined,
      size: undefined,
      spin: undefined };

    mountedIcon = undefined;
  });

  it('always renders italics', function () {
    var italics = icon().find("i");
    expect(italics.length).toBeGreaterThan(0);
  });
});